{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "geist-ui-components",
  "title": "Geist UI Components",
  "description": "Links to the components documentation in Geist UI",
  "icon": "command-icon.png",
  "author": "RahulatRaycast",
  "license": "MIT",
  "commands": [
    {
      "name": "general",
      "title": "General",
      "description": "General components in Geist UI",
      "mode": "view"
    },
    {
      "name": "layout",
      "title": "Layout",
      "description": "Layout components in Geist UI",
      "mode": "view"
    },
    {
      "name": "surfaces",
      "title": "Surfaces",
      "description": "Components that fall under surfaces",
      "mode": "view"
    },
    {
      "name": "data-entry",
      "title": "Data Entry",
      "description": "Data Entry components in Geist UI",
      "mode": "view"
    },
    {
      "name": "data-display",
      "title": "Data Display",
      "description": "Data display components in Geist UI",
      "mode": "view"
    },
    {
      "name": "feedback",
      "title": "Feedback",
      "description": "Feedback components in Geist UI",
      "mode": "view"
    },
    {
      "name": "navigation",
      "title": "Navigation",
      "description": "Navigation components in Geist UI",
      "mode": "view"
    },
    {
      "name": "others",
      "title": "Others",
      "description": "Random components in Geist UI",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}